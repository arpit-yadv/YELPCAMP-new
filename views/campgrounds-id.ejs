<%- include('partials/header') %>

<div class="container">
    <div class="card mb-3 bg-dark text-white">
        <h1 class="display-3 text-center my-4 "><%= campground.title %></h1>
        <img src="<%= campground.image %>" class="img-fluid m-3 rounded " alt="Responsive image">
        <div class="card-body">
          <h5 class="card-title"><%= campground.title %></h5>
          <a href="/campgrounds/<%=campground._id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/campgrounds" class="btn btn-danger">Delete</a>
          <p class="card-text"><%= campground.price %>/- INR</p>
          <p class="card-text"><%= campground.description %></p>
          <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
        </div>
    </div>
</div>







<%- include('partials/footer') %>